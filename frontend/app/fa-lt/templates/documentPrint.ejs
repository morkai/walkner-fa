<!DOCTYPE html>
<html>
<head>
  <title><%- details.documentNo %></title>
  <style>
    * {
      margin: 0;
      padding: 0;
      line-height: 1;
      box-sizing: border-box;
    }
    body {
      font-size: 16px;
    }
    .page {
      margin: 0.75cm 0.75cm 0 0.75cm;
      page-break-after: always;
    }
    .page:last-child {
      page-break-after: unset;
    }
    h1 {
      margin-bottom: 0.5cm;
      border-bottom: 4px solid #000;
      font-size: 32px;
      line-height: 1.4;
    }
    .text-fixed {
      font-family: monospace;
    }
    .text-lines {
      white-space: pre-line;
    }
    table {
      width: 100%;
      border-collapse: collapse;
      line-height: 1.4;
    }
    th {
      padding: 0.15cm 0;
      width: 1%;
      text-align: right;
      vertical-align: top;
      white-space: nowrap;
    }
    td {
      padding: 0.15cm 0 0.15cm 0.25cm;
    }
    .comment {
      page-break-inside: avoid;
      font-size: 12px;
      line-height: 1.4;
    }
    .comment + .comment {
      margin-top: 0.25cm;
      padding-top: 0.25cm;
      border-top: 1px solid #000;
    }
    .comment-text {
      white-space: pre-line;
    }
  </style>
</head>
<body>
<div class="page">
<h1 style="display: flex">
  <span><%- t('print:document:title', {kind: model.kind}) %></span>
  <span style="margin-left: auto; font-size: 10px; text-align: center">
    <img height="25" src="data:image/svg+xml;base64," alt="Signify">
    <br>
    <%- t('fa-common', 'print:division') %>
  </span>
</h1>
<table>
  <tbody>
  <tr>
    <th><%- t('PROPERTY:kind') %></th>
    <td><%- details.kind %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:protocolNo') %></th>
    <td class="text-fixed"><%- details.protocolNo %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:protocolDate') %></th>
    <td><%- details.protocolDate %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:documentNo') %></th>
    <td class="text-fixed"><%- details.documentNo %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:documentDate') %></th>
    <td><%- details.documentDate %></td>
  </tr>
  <% if (model.kind === 'merge') { %>
    <tr>
      <th><%- t('PROPERTY:mergeInventoryNo') %></th>
      <td class="text-fixed"><%= details.mergeInventoryNo %></td>
    </tr>
    <tr>
      <th><%- t('PROPERTY:mergeLineSymbol') %></th>
      <td class="text-fixed"><%= details.mergeLineSymbol %></td>
    </tr>
    <tr>
      <th><%- t('PROPERTY:mergeType') %></th>
      <td><%- details.mergeType %></td>
    </tr>
  <% } %>
  <tr>
    <th><%- t('PROPERTY:assetName') %></th>
    <td><%= details.assetName %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:inventoryNo') %></th>
    <td class="text-fixed"><%= details.inventoryNo %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:costCenter') %></th>
    <td class="text-fixed"><%= details.costCenter %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:owner') %></th>
    <td><%- details.owner %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:applicant') %></th>
    <td><%- details.applicant %></td>
  </tr>
  <% if (details.committee.length) { %>
  <tr>
    <th><%- t('PROPERTY:committee') %></th>
    <td>
      <% details.committee.forEach(function(user, i) { %>
        <%- i + 1 %>. <%- user %><br>
      <% }) %>
    </td>
  </tr>
  <% } %>
  <tr>
    <th><%- t('PROPERTY:cause') %></th>
    <td class="text-lines"><%= details.cause %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:initialValue') %></th>
    <td><%= details.initialValue %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:deprecationValue') %></th>
    <td><%= details.deprecationValue %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:netValue') %></th>
    <td><%= details.netValue %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:economicInitialValue') %></th>
    <td><%= details.economicInitialValue %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:economicDeprecationValue') %></th>
    <td><%= details.economicDeprecationValue %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:economicNetValue') %></th>
    <td><%= details.economicNetValue %></td>
  </tr>
  <tr>
    <th><%- t('PROPERTY:assets.no') %></th>
    <td class="text-fixed"><%= details.assetNos %></td>
  </tr>
  <% if (model.kind !== 'scrap' && model.kind !== 'sale') { %>
  <tr>
    <th><%- t('PROPERTY:accountingNo') %></th>
    <td class="text-fixed"><%= details.accountingNos %></td>
  </tr>
  <% } %>
  </tbody>
</table>
</div>
<% if (comments.length) { %>
<div class="page">
  <h1 style="display: flex">
    <span><%- t('print:document:title', {kind: model.kind}) %></span>
    <span style="margin-left: auto; font-size: 10px; text-align: center">
    <img height="25" src="data:image/svg+xml;base64," alt="Signify">
    <br>
    <%- t('fa-common', 'print:division') %>
  </span>
  </h1>
  <% comments.forEach(function(comment) { %>
  <div class="comment">
    <span class="comment-info"><%- comment.user %> @ <%- comment.time %>:</span>
    <span class="comment-text"><%= comment.text %></span>
  </div>
  <% }) %>
</div>
<% } %>
</body>
</html>
