<div class="fa-edit-stage" data-stage="verify">
  <%- helpers.props(details, [
    '!kind',
    {id: 'mergeInventoryNo', visible: model.kind === 'merge', valueClassName: 'text-fixed'},
    {id: 'mergeLineSymbol', visible: model.kind === 'merge', valueClassName: 'text-fixed'},
    {id: 'mergeType', visible: model.kind === 'merge'},
    {id: 'protocolNo', valueClassName: 'text-fixed'},
    'protocolDate',
    'assetName',
    {id: 'inventoryNo', valueClassName: 'text-fixed'},
    {id: 'costCenter', valueClassName: 'text-fixed'},
    {id: '!owner'},
    {id: '!applicant'},
    helpers.fa.committeeProp(),
    {id: 'cause', valueClassName: 'text-lines'}
  ]) %>
  <div class="form-group">
    <label for="<%- idPrefix %>-assetName" class="control-label is-required"><%- t('PROPERTY:assetName') %></label>
    <input id="<%- idPrefix %>-assetName" name="assetName" class="form-control fa-edit-long" type="text" data-required data-maxlength="50 200">
  </div>
  <div class="fa-edit-row">
    <div id="<%- idPrefix %>-initialValue" style="margin: 0 15px 15px 0"></div>
    <div id="<%- idPrefix %>-economicInitialValue" style="margin-bottom: 15px"></div>
  </div>
  <div class="fa-edit-row">
    <div id="<%- idPrefix %>-deprecationValue" style="margin: 0 15px 15px 0"></div>
    <div id="<%- idPrefix %>-economicDeprecationValue" style="margin-bottom: 15px"></div>
  </div>
  <div class="fa-edit-row">
    <div id="<%- idPrefix %>-netValue" style="margin: 0 15px 15px 0"></div>
    <div id="<%- idPrefix %>-economicNetValue" style="margin-bottom: 15px"></div>
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-sapNo" class="control-label is-required"><%- t('PROPERTY:sapNo') %></label>
    <input id="<%- idPrefix %>-sapNo" name="sapNo" class="form-control fa-edit-short" type="text" data-required maxlength="50">
  </div>
  <%- helpers.fa.attachmentFormGroup({name: 'attachment'}) %>
  <div class="form-group">
    <label for="<%- idPrefix %>-comment" class="control-label"><%- t('PROPERTY:comment') %></label>
    <textarea id="<%- idPrefix %>-comment" class="form-control" name="comment" rows="3"></textarea>
  </div>
</div>
