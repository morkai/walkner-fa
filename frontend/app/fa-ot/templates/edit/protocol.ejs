<div class="fa-edit-stage" data-stage="protocol">
  <div class="form-group">
    <label class="control-label"><%- t('PROPERTY:commissioningType') %></label>
    <p class="form-control-static"><%- t('commissioningType:' + commissioningType) %></p>
  </div>
  <div class="form-group fa-edit-short">
    <label for="<%- idPrefix %>-protocolDate" class="control-label is-required"><%- t('PROPERTY:protocolDate') %></label>
    <input id="<%- idPrefix %>-protocolDate" name="protocolDate" class="form-control fa-edit-long" type="date" data-required>
  </div>
  <% if (commissioningType === 'inc-asset') { %>
  <div class="form-group fa-edit-short">
    <label for="<%- idPrefix %>-inventoryNo" class="control-label is-required"><%- t('PROPERTY:inventoryNo') %></label>
    <input id="<%- idPrefix %>-inventoryNo" name="inventoryNo" class="form-control" type="text" data-required>
  </div>
  <% } %>
  <div class="fa-edit-row fa-edit-long">
    <div class="form-group fa-edit-flex" style="margin-bottom: 15px">
      <label for="<%- idPrefix %>-assetName" class="control-label is-required"><%- t('PROPERTY:assetName') %></label>
      <input id="<%- idPrefix %>-assetName" name="assetName" class="form-control" type="text" data-required>
    </div>
    <% if (usageDestination === 'factory') { %>
    <div class="form-group fa-edit-short">
      <label for="<%- idPrefix %>-lineSymbol" class="control-label"><%- t('PROPERTY:lineSymbol') %></label>
      <input id="<%- idPrefix %>-lineSymbol" name="lineSymbol" class="form-control" type="text">
    </div>
    <% } %>
  </div>
  <div id="<%- idPrefix %>-zplx"></div>
  <p class="fa-ot-protocol-statement">
    <span class="fa-ot-protocol-statement-hd"><%- t('protocol:statement:hd') %></span>
    <span class="fa-ot-protocol-statement-bd"><%- t('protocol:statement:bd') %></span>
  </p>
  <div id="<%- idPrefix %>-participants" style="margin-bottom: 15px"></div>
  <% if (commissioningType === 'new-asset') { %>
  <div class="form-group fa-edit-long" style="margin-top: -10px">
    <label for="<%- idPrefix %>-supplier" class="control-label is-required"><%- t('PROPERTY:supplier') %></label>
    <input id="<%- idPrefix %>-supplier" name="supplier" class="form-control" type="text" data-required>
  </div>
  <% } %>
  <div class="form-group fa-edit-long" style="margin-top: <%- commissioningType === 'inc-asset' ? '-10px' : '0' %>">
    <label for="<%- idPrefix %>-protocolFile" class="control-label"><%- t('PROPERTY:protocolFile') %></label>
    <table class="fa-edit-attachment">
      <tbody>
      <tr>
        <td class="is-min" style="padding: 0 10px 5px 0"><%- t('FORM:edit:attachment:old') %></td>
        <td style="padding-bottom: 5px">
          <% if (protocolFile) { %>
          <a href="/fa/ot/<%- documentId %>/attachments/protocol" target="_blank"><%= protocolFile.name %></a>
          <% } else { %>
          <%- t('FORM:edit:attachment:empty') %>
          <% } %>
        </td>
      </tr>
      <tr>
        <td class="is-min" style="padding-right: 10px"><%- t('FORM:edit:attachment:new') %></td>
        <td><input id="<%- idPrefix %>-protocolFile" name="protocolFile" class="form-control" type="file"></td>
      </tr>
      </tbody>
    </table>
    <% if (protocolFile) { %>
    <p class="help-block"><%- t('FORM:edit:attachment:help') %></p>
    <% } %>
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-comment" class="control-label"><%- t('PROPERTY:comment') %></label>
    <textarea id="<%- idPrefix %>-comment" class="form-control" name="comment" rows="3"></textarea>
  </div>
</div>
