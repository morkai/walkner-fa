<div class="fa-edit-stage" data-stage="protocol">
  <%- helpers.props(details, [
    '!protocolNeeded',
    '!commissioningType',
    {id: '!extendedDep', visible: model.commissioningType === 'inc-asset'},
    '!usageDestination',
    {id: 'protocolNo', visible: model.protocolNeeded, valueClassName: 'text-fixed'}
  ]) %>
  <div class="form-group">
    <label for="<%- id('protocolDate') %>" class="control-label is-required"><%- t('PROPERTY:protocolDate') %></label>
    <input id="<%- id('protocolDate') %>" name="protocolDate" class="form-control fa-edit-short" type="date" data-required>
  </div>
  <div id="<%- id('zplx') %>"></div>
  <div id="<%- id('assets')%>"></div>
  <%- helpers.fa.attachmentFormGroup({
    name: 'protocol',
    required: true,
    accept: ['doc', 'img']
  }) %>
  <div class="form-group">
    <label for="<%- id('comment') %>" class="control-label"><%- t('PROPERTY:comment') %></label>
    <textarea id="<%- id('comment') %>" class="form-control" name="comment" rows="3"></textarea>
  </div>
</div>
